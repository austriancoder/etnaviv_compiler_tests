FRAG
PROPERTY FS_COLOR0_WRITES_ALL_CBUFS 1
DCL IN[0], GENERIC[9], PERSPECTIVE
DCL IN[1], GENERIC[10], PERSPECTIVE
DCL IN[2], GENERIC[11], PERSPECTIVE
DCL IN[3], GENERIC[12], PERSPECTIVE
DCL OUT[0], COLOR
DCL CONST[0]
DCL TEMP[0], LOCAL
DCL ADDR[0]
