FRAG
PROPERTY FS_COLOR0_WRITES_ALL_CBUFS 1
DCL IN[0], GENERIC[0], PERSPECTIVE
DCL OUT[0], COLOR
DCL TEMP[0], LOCAL
  1: MOV TEMP[0], IN[0]
  2: IF IN[0].xxxx :0
  3:   ADD TEMP[0], TEMP[0], TEMP[0]
  4:   KILL
  5: ENDIF
  6: IF IN[0].yyyy :0
  7:   MUL TEMP[0], TEMP[0], TEMP[0]
  8:   KILL
  9: ENDIF
 11: MOV OUT[0], TEMP[0]
 12: END
