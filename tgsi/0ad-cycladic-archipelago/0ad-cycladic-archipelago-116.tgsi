FRAG
PROPERTY FS_COLOR0_WRITES_ALL_CBUFS 1
DCL IN[0], GENERIC[9], PERSPECTIVE
DCL OUT[0], COLOR
DCL SAMP[0]
DCL CONST[0..1]
DCL TEMP[0], LOCAL
  0: MOV TEMP[0].xy, IN[0].xyyy
  1: TEX TEMP[0], TEMP[0], SAMP[0], 2D
  2: ADD TEMP[0], TEMP[0], CONST[0]
  3: MUL TEMP[0], TEMP[0], CONST[1]
  4: MOV OUT[0], TEMP[0]
  5: END
