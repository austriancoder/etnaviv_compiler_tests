VERT
DCL IN[0]
DCL IN[1]
DCL OUT[0], POSITION
DCL OUT[1], GENERIC[9]
DCL CONST[0..3]
DCL TEMP[0..1], LOCAL
  0: MUL TEMP[0], CONST[0], IN[1].xxxx
  1: MAD TEMP[0], CONST[1], IN[1].yyyy, TEMP[0]
  2: MAD TEMP[0], CONST[2], IN[1].zzzz, TEMP[0]
  3: ADD TEMP[0], TEMP[0], CONST[3]
  4: MOV TEMP[1].xyz, IN[0].xyzx
  5: MOV OUT[0], TEMP[0]
  6: MOV OUT[1], TEMP[1]
  7: END
