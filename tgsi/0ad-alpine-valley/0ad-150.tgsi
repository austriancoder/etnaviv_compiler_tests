VERT
DCL IN[0]
DCL IN[1]
DCL OUT[0], POSITION
DCL OUT[1], GENERIC[9]
DCL CONST[0..3]
DCL TEMP[0..1], LOCAL
  0: MUL TEMP[0], CONST[0], IN[0].xxxx
  1: MAD TEMP[0], CONST[1], IN[0].yyyy, TEMP[0]
  2: ADD TEMP[0], TEMP[0], CONST[3]
  3: MOV TEMP[1].xyz, IN[1].xyzx
  4: MOV OUT[1], TEMP[1]
  5: MOV OUT[0], TEMP[0]
  6: END
